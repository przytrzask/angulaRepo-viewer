!!!
%html
  %head
    %title githubAPI
    %link{ :href=> '/../node_modules/angular-material/angular-material.css', :rel=> 'stylesheet', :type=>'text/css'}
  %body{:layout => "column", "ng-app" => "starterApp", :"ng-cloak" => ""}
    %script{:src=>'/../node_modules/angular/angular.js', :type=>'text/javascript'}
    %script{:src=>'/../node_modules/angular-animate/angular-animate.js', :type=>'text/javascript'}
    %script{:src=>'/../node_modules/angular-aria/angular-aria.js', :type=>'text/javascript'}
    %script{:src=>'/../node_modules/angular-material/angular-material.js', :type=>'text/javascript'}
    %script{:src=>'/../node_modules/angular-route/angular-route.js', :type=>'text/javascript'}
    %script{:src=>'./js/material-design_settings.js', :type=>'text/javascript'} 
    
    %div{:'ng-controller'=>'navController'}
      %md-content.md-padding
        %md-nav-bar{:'md-selected-nav-item'=>'currentNavItem', :'nav-bar-aria-label'=>'navigation links'}
          %md-nav-item{ :'md-nav-href'=> "#app.page1", :'name'=>"home"}
            %a{:'ng-click'=>"getData()"}home
          %md-nav-item{:'md-nav-href'=> "#app.page2", :'name'=>"repos"}repositories
          %md-nav-item{:'md-nav-href'=> "#app.page3", :'name'=>"users"}Users
          %md-nav-item{:'md-nav-click'=>"goto('page3')", :name=>"page3"}Page Three
          %p {{currentNavItem}}
  
    %div{:flex => "", :layout => "row"}
      %md-sidenav.md-whiteframe-z2{"md-is-locked-open" => "true"}
        %div{:'layout'=>'column'}
          %img{:src=>'./assets/img/angular-logo.svg', :class=>"md-card-image", :alt=>"Washed Out"}
      %md-content#content{:flex => ""}
        %md-progress-circular{:'md-mode'=>"indeterminate", :'ng-show'=>"isLoading"}
        %div{:'ng-controller'=>'navController',:class=>'md-padding',:'layout'=>"row",:'layout-wrap'=>''}
          %md-card{:'style'=>'width: 150px',:'ng-repeat'=>'data in mydata track by data.name'}
            %img{:src=>'./assets/img/angular-logo.svg', :class=>"md-card-image", :alt=>"Washed Out"}
              %md-card-title
                %md-card-title-text {{data.name}} 
          